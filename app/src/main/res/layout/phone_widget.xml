<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/d_wedgit"
    style="@style/fd"
    android:divider="#00000000"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <!--android:divider="#9146050b"
        android:dividerPadding="2dp"-->
    <LinearLayout
        android:id="@+id/d_share"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/selector_base"
        android:divider="#00000000"
        android:clickable="true"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:minHeight="25dp"
        android:minWidth="25dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/s_share"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"

            android:scaleType="centerInside"
            android:src="?attr/d1" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/textView1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:labelFor="@id/s_share"
            android:text="Share"
            android:textSize="10dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/d_delete"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/selector_base"
        android:clickable="true"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:minHeight="25dp"
        android:minWidth="25dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/s_delete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:clickable="false"
            android:longClickable="false"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"

            android:scaleType="centerInside"
            android:src="?attr/d2" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/calls_number_text"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:labelFor="@id/s_delete"
            android:text="Delete"
            android:textSize="10dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/d_minfo"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/selector_base"
        android:clickable="true"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:minHeight="25dp"
        android:minWidth="25dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/s_moreinfo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:clickable="false"
            android:longClickable="false"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"

            android:scaleType="centerInside"
            android:src="?attr/d3" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:labelFor="@id/s_moreinfo"
            android:text="Info"
            android:textSize="10dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/d_edit"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/selector_base"
        android:clickable="true"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:minHeight="25dp"
        android:minWidth="25dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/s_edit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:clickable="false"
            android:longClickable="false"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"
            android:scaleType="centerInside"

            android:src="?attr/d4"
            android:visibility="visible" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/calls_dur_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:labelFor="@id/s_edit"
            android:text="Edit"
            android:textSize="10dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/d_sendbla"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="@drawable/selector_base"
        android:clickable="true"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:minHeight="25dp"
        android:minWidth="25dp"
        android:orientation="vertical">

        <ImageView
            android:id="@+id/s_blance"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:adjustViewBounds="true"
            android:clickable="false"
            android:labelFor="@id/s_blance"
            android:longClickable="false"
            android:paddingLeft="15dp"
            android:paddingRight="15dp"

            android:scaleType="centerInside"
            android:src="?attr/d5" />

        <android.support.v7.widget.AppCompatTextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal"
            android:text="Send blance"
            android:textSize="10dp" />
    </LinearLayout>
</LinearLayout>