<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="d1" format="reference" />
    <attr name="d2" format="reference" />
    <attr name="d3" format="reference" />
    <attr name="d4" format="reference" />
    <attr name="d5" format="reference" />
    <attr name="d6" format="reference" />
    <attr name="send_sms" format="reference"/>
    <attr name="make_call" format="reference"/>
    <attr name="expand" format="reference"/>
    <attr name="history" format="reference"/>
    <attr name="messges" format="reference"/>
    <attr name="p_net_text_color" format="color"/>
    <declare-styleable name="AZSideBar">
        <attr name="shape_background" format="color" />
        <attr name="selector_color" format="color" />
        <attr name="text_color" format="color" />
        <attr name="text_selected_color" format="color" />
        <attr name="enable_animateion" format="boolean" />


    </declare-styleable>
    <declare-styleable name="HeaderTabs">
        <attr name="net1" format="reference" />
        <attr name="net2" format="reference" />
        <attr name="net3" format="reference" />


    </declare-styleable>
    <declare-styleable name="FontTextView">
        <attr name="font" format="string" />

        <attr name="efont" format="enum">
            <enum name="as.ttf" value="0" />
            <enum name="Ba.ttf" value="1" />
            <enum name="bein.ttf" value="2" />
            <enum name="bset.otf" value="3" />
            <enum name="kofi.ttf" value="4" />
            <enum name="mrb33.ttf" value="5" />

        </attr>
    </declare-styleable>
    <declare-styleable name="SlidingUpPanelLayout">
        <attr name="panelHeight" format="dimension" />
        <attr name="shadowHeight" format="dimension" />
        <attr name="paralaxOffset" format="dimension" />
        <attr name="fadeColor" format="color" />
        <attr name="flingVelocity" format="integer" />
        <attr name="dragView" format="reference" />
        <attr name="overlay" format="boolean" />
        <attr name="anchorPoint" format="float" />
    </declare-styleable>
    <declare-styleable name="LetterImageView">
        <attr name="randomColor" format="boolean" />

        <attr name="fchar" format="string" />
    </declare-styleable>


</resources>